<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="NoEffectLayout">
        <attr name="animEnable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BorderEffectLayout">
        <attr name="effectShape" format="integer">
            <!-- default rectangle -->
            <enum name="rectangle" value="0" />
            <enum name="oval" value="1" />
        </attr>
    </declare-styleable>

</resources>